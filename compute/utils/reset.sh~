#!/bin/bash


cp job_starter /var/spool/torque/mom_priv

rm -f /var/spool/torque/mom_priv/prologue
rm -f /var/spool/torque/mom_priv/prologue.parallel
rm -f /var/spool/torque/mom_priv/epilogue
rm -f /var/spool/torque/mom_priv/epilogue.parallel

cp -fr job_starter_null /var/spool/torque/mom_priv/job_starter


